{
  "setupFiles": ["<rootDir>/src/setupTests.js"],
  "testRegex": "/*.test.js$",
  "collectCoverage": true,
  "coverageReporters": ["lcov"],
  "coverageDirectory": "test-coverage",
  "testTimeout": 10000,
  "maxRetries": 3,
  "coverageThreshold": {
   "global": {
   "branches": 50,
   "functions": 50,
   "lines": 50,
   "statements": 50
   }
  },
  "moduleDirectories": ["node_modules", "src"]
 }


